#include <Arduino.h>
#include <LiquidCrystal.h>

#define pin_rs 38
#define pin_e  14
#define pin_d4 39
#define pin_d5 21
#define pin_d6 45
#define pin_d7 47

LiquidCrystal lcd(pin_rs, pin_e, pin_d4, pin_d5, pin_d6, pin_d7);

void setup() {
  lcd.begin(16, 2);
  lcd.setCursor(0, 0);
  lcd.print("Pot1  Pot2  Cont");
  lcd.setCursor(0, 1);
  for (int i = 0; i < 16; i++) lcd.print(' ');
}

void loop() {
}
